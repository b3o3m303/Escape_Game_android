<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.escape.escape_new.finish">

    <RelativeLayout
        android:layout_width="576dp"
        android:layout_height="271dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        android:background="@drawable/finish">

        <Button
            android:id="@+id/new1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="11dp"
            android:layout_marginStart="135dp"
            android:background="#00000000"
            android:text="new_Game" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/new1"
            android:layout_alignBottom="@+id/new1"
            android:layout_marginStart="131dp"
            android:layout_toEndOf="@+id/new1"
            android:background="#00000000"
            android:text="about_us" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
